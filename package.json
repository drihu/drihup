{
  "name": "drihup",
  "version": "0.1.0",
  "description": "A simple skeleton application with Express and Pug",
  "private": true,
  "main": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack",
    "watch": "npm-run-all --parallel watch:*",
    "watch:app": "nodemon -L --ignore resources/assets --ignore public/assets ./bin/www",
    "watch:wp": "webpack --watch"
